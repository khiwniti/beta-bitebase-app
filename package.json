{
  "name": "bitebase-geospatial-saas",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:frontend": "turbo run dev --filter=bitebase-frontend",
    "start": "turbo run start",
    "start:frontend": "turbo run start --filter=bitebase-frontend",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "setup": "npm install"
  },

  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
