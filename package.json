{
  "name": "bitebase-geospatial-saas",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:frontend": "turbo run dev --filter=bitebase-frontend",
    "dev:api": "turbo run dev --filter=bitebase-api",
    "dev:backend": "turbo run dev --filter=bitebase-backend-production",
    "start": "turbo run start",
    "start:frontend": "turbo run start --filter=bitebase-frontend",
    "start:api": "turbo run start --filter=bitebase-api",
    "start:backend": "turbo run start --filter=bitebase-backend-production",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "install:api": "cd apps/api && pip install -r requirements.txt",
    "setup": "npm install && npm run install:api"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "client-only": "^0.0.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^11.8.1",
    "form-data": "^4.0.2",
    "lucide-react": "^0.503.0",
    "next": "15.4.0-canary.50",
    "nvm": "^0.0.4",
    "readline": "^1.3.0",
    "styled-jsx": "^5.1.6"
  },
  "devDependencies": {
    "@swc/helpers": "^0.5.1",
    "@types/mapbox__vector-tile": "^1.3.4",
    "prettier": "^3.5.3",
    "tempo-devtools": "^2.0.106",
    "turbo": "^2.5.1",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
